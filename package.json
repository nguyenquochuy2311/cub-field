	"scripts": {
		"serve": "npm run build && node dist/server.js",
		"start": "nodemon --signal SIGTERM",
		"build": "swc src -d dist --strip-leading-paths --source-maps --copy-files",
		"tsc": "tsc --noEmit && tsc-alias && eslint src --ext .ts",
		"tsc:fix": "eslint src --ext .ts --fix",
		"migrate": "npm run build && node dist/scripts/migrate --all",
		"migrate:ts": "ts-node -r tsconfig-paths/register src/scripts/migrate --all",
		"migrate:generate": "ts-node src/scripts/umzug create --name ${npm_config_name}.ts"
	},
	"lint-staged": {
		"**/*.ts": "npm run tsc"
	},
